{
  "name": "react-buffet-examples",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "radium": "0.18.1",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "fix:static:url": "grep -lr --exclude='./build/\\.*' -e 'static' ./build | xargs sed -i '' 's/\\/static/\\/react-buffet\\/static/g'",
    "fix:favicon:url": "sed -i '' 's/\\/favicon/favicon/g' ./build/index.html",
    "fix:urls": "npm run fix:static:url && npm run fix:favicon:url",
    "unfoldbuild": "cp -r ./build/* . && rm -R ./build",
    "build": "react-scripts build",
    "postbuild": " npm run fix:urls && npm run unfoldbuild"
  }
}
